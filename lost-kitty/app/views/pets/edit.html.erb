<%= form_with model: @pet do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :gender %>
  <%= f.text_field :gender %>
  <%= f.label :species %>
  <%= f.select :species, collection: Pet::SPECIES %>
  <%= f.label :injured %>
  <%= f.check_box :injured %>
  <%= f.label :age %>
  <%= f.number_field :age %>
  <%= f.label :found_on %>
  <%= f.date_field :found_on %>
  <%= f.submit  %>
<% end %>
